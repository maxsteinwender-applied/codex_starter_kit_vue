{
  "name": "codex-starter-kit-vue",
  "version": "1.0.0",
  "description": "Codex-first Nuxt starter kit with explicit markdown agents and playbooks",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "nuxt prepare",
    "dev": "nuxt dev --host 127.0.0.1 --port 3000",
    "build": "nuxt build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint . --ext .js,.mjs,.cjs",
    "format": "eslint . --ext .js,.mjs,.cjs --fix",
    "typecheck": "nuxt typecheck",
    "test": "npm run test:unit",
    "test:unit": "nuxt prepare && vitest run",
    "test:unit:watch": "nuxt prepare && vitest",
    "test:e2e": "npm run build && playwright test",
    "test:e2e:install": "playwright install --with-deps chromium",
    "doctor": "node scripts/doctor.mjs",
    "check": "npm run lint && npm run typecheck && npm run build",
    "check:ci": "npm run doctor && npm run validate:docs && npm run check && npm run test:unit",
    "validate:docs": "node scripts/validate-doc-contracts.mjs"
  },
  "dependencies": {
    "@primevue/nuxt-module": "^4.4.1",
    "@supabase/supabase-js": "^2.39.3",
    "chart.js": "^4.5.1",
    "nuxt": "^3",
    "primevue": "^4.4.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@playwright/test": "^1.58.2",
    "@types/node": "^20",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "jsdom": "^27.4.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.2.4"
  }
}
